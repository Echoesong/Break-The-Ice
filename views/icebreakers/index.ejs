<%- include("../partials/header", {title}) %>

<section>
    <table class="table table-striped">
        <thead>
            <tr>
                <th>Icebreaker</th>
                <th class="center">Topic</th>
                <th class="center"># of Answers</th>
                <th class="center">Details</th>
            </tr>
        </thead>
        <tbody>
            <% allIcebreakers.forEach(function(i) { %>
            <tr>
                <td><%= i.content %></td>
                <td class="center"><%= i.topic %></td>
                <td class="center"><%= i.answers.length%></td>
                <td class="center"><a href="/icebreakers/<%=i._id%> ">See Answers</a></td>
            </tr>
            <% })%>
        </tbody>
    </table>
</section>

<%- include("../partials/footer") %>